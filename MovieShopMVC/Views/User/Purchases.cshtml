@model IEnumerable<ApplicationCore.Models.MovieCardResponseModel>

@{
    ViewData["Title"] = "Purchase";
}

<h1>Purchases</h1>

<div class="rounded">
    <div class="container-fluid">
        <div class="row">
            @foreach (var movieCard in Model)
            {
                <div class="col-xl-2 col-sm-4 col-lg-3 col-6">
                    <partial name="_movieCard" model="movieCard"/>
                </div>
            }
        </div>
    </div>
</div>